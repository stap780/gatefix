<td><%= order.created_at.strftime("%d/%m/%Y %H:%M")%></td>
<td><%= order.transaction_id %></td>
<td><%= order.amount %></td>
<td><%= order.description %></td>
<td><%= order.order_id %></td>
<td><%= order.phone %><br><%= order.email %></td>
<td><%= order.status %></td>
<td><%= link_to '<span class="oi oi-eye"></span>'.html_safe, order %></td>
<td><%= link_to '<span class="oi oi-pencil"></span>'.html_safe, edit_order_path(order) %></td>
<td><%= link_to '<span class="oi oi-trash"></span>'.html_safe, order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
